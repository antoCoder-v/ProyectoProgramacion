<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane fx:id="switchPane" prefHeight="30.0" prefWidth="36.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectodeprogramacion.ControladorSwitch">
    <children>
        <Pane fx:id="mainPane" layoutY="4.0" prefHeight="34.0" prefWidth="36.0">
            <children>
                <Rectangle fx:id="rectangle" arcHeight="5.0" arcWidth="5.0" fill="#878c91" height="35.0" layoutX="1.0" stroke="BLACK" strokeType="INSIDE" width="35.0" />
                <Circle fx:id="circle4" layoutX="7.0" layoutY="6.0" radius="4.0" stroke="BLACK" strokeType="INSIDE" />
                <Circle fx:id="circle3" layoutX="7.0" layoutY="29.0" radius="4.0" stroke="BLACK" strokeType="INSIDE" />
                <Circle fx:id="circle1" layoutX="29.0" layoutY="6.0" radius="4.0" stroke="BLACK" strokeType="INSIDE" />
                <Circle fx:id="circleCenter" layoutX="18.0" layoutY="18.0" radius="9.0" stroke="BLACK" strokeType="INSIDE">
                    <fill>
                        <LinearGradient endX="1.0" endY="1.0">
                            <stops>
                                <Stop>
                                    <color>
                                        <Color />
                                    </color>
                                </Stop>
                                <Stop offset="1.0">
                                    <color>
                                        <Color red="1.0" green="1.0" blue="1.0" />
                                    </color>
                                </Stop>
                            </stops>
                        </LinearGradient>
                    </fill>
                </Circle>
                <Circle fx:id="circle2" layoutX="30.0" layoutY="29.0" radius="4.0" stroke="BLACK" strokeType="INSIDE" />
                <Button fx:id="encender" layoutX="9.0" layoutY="3.0" mnemonicParsing="false" opacity="0.16" prefHeight="9.0" prefWidth="18.0" />
            </children>
        </Pane>
    </children>
</AnchorPane>