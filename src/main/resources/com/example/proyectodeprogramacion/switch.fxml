<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane fx:id="switchPane" prefHeight="28.0" prefWidth="30.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectodeprogramacion.ControladorSwitch">
    <children>
        <Pane fx:id="mainPane" layoutX="-1.0" layoutY="-2.0" prefHeight="30.0" prefWidth="32.0">
            <children>
                <Rectangle fx:id="rectangle" arcHeight="5.0" arcWidth="5.0" fill="#878c91" height="30.0" layoutX="2.0" layoutY="2.0" stroke="BLACK" strokeType="INSIDE" width="30.0" />
                <Circle fx:id="circle4" layoutX="8.0" layoutY="8.0" radius="3.0" stroke="BLACK" strokeType="INSIDE" />
                <Circle fx:id="circle3" layoutX="8.0" layoutY="26.0" radius="3.0" stroke="BLACK" strokeType="INSIDE" />
                <Circle fx:id="circle1" layoutX="26.0" layoutY="8.0" radius="3.0" stroke="BLACK" strokeType="INSIDE" />
                <Circle fx:id="circleCenter" layoutX="17.0" layoutY="17.0" radius="7.0" stroke="BLACK" strokeType="INSIDE">
                    <fill>
                        <LinearGradient endX="1.0" endY="1.0">
                            <stops>
                                <Stop>
                                    <color>
                                        <Color />
                                    </color>
                                </Stop>
                                <Stop offset="1.0">
                                    <color>
                                        <Color red="1.0" green="1.0" blue="1.0" />
                                    </color>
                                </Stop>
                            </stops>
                        </LinearGradient>
                    </fill>
                </Circle>
                <Circle fx:id="circle2" layoutX="26.0" layoutY="26.0" radius="3.0" stroke="BLACK" strokeType="INSIDE" />
                <Button fx:id="encender" layoutX="9.0" layoutY="3.0" mnemonicParsing="false" opacity="0.16" prefHeight="3.0" prefWidth="14.0" />
            </children>
        </Pane>
    </children>
</AnchorPane>
